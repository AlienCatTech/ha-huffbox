{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"id": {
			"type": "string"
		},
		"width": {
			"type": "string"
		},
		"component": {
			"type": "string"
		},
		"props": {
			"type": "object",
			"additionalProperties": true
		},
		"nodeClass": {
			"type": "string"
		},
		"wrapperClass": {
			"type": "string"
		},
		"layoutClass": {
			"type": "string"
		},
		"cols": {
			"type": "array",
			"items": {
				"$ref": "#"
			}
		},
		"rows": {
			"type": "array",
			"items": {
				"$ref": "#"
			}
		},
		"posX": {
			"type": "string",
			"enum": ["left", "right", "middle"]
		},
		"posY": {
			"type": "string",
			"enum": ["top", "bottom", "middle"]
		},
		"alignHeight": {
			"type": "boolean"
		},
		"wrap": {
			"type": "string",
			"enum": ["wrap", "nowrap"]
		},
		"gap": {
			"type": "string"
		}
	},
	"additionalProperties": false
}
